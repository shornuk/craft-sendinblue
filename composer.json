{
    "name": "shornuk/craft-sendinblue",
    "description": "Integration with SendinBlue API. Create, update and delete sendinblue Contacts from with Craft",
    "type": "craft-plugin",
    "version": "0.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "sendinblue"
    ],
    "support": {
        "docs": "https://github.com/shornuk/craft-sendinblue/blob/master/README.md",
        "issues": "https://github.com/shornuk/craft-sendinblue/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Hill",
            "homepage": "https://shorn.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "sendinblue/api-v3-sdk": "^6.1.0"
    },
    "autoload": {
        "psr-4": {
          "shornuk\\sendinblue\\": "src/"
        }
    },
    "extra": {
        "name": "SendinBlue",
        "handle": "sendinblue",
        "developer": "Sean Hill",
        "developerUrl": "shorn.co.uk",
        "documentationUrl": "https://github.com/shornuk/craft-sendinblue/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/shornuk/craft-sendinblue/master/CHANGELOG.md",
        "components": {
            "sendinBlueService": "shornuk\\sendinblue\\services\\SendinBlueService"
        },
        "class": "shornuk\\sendinblue\\SendinBlue"
    }
}
